<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>{% block title %} PRISK Admin Portal{% endblock %}</title>
    <meta name="description" content="Admin, Dashboard, Bootstrap, Bootstrap 4, Angular, AngularJS" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, minimal-ui" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <!-- for ios 7 style, multi-resolution icon of 152x152 -->
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-barstyle" content="black-translucent">
    <link rel="apple-touch-icon" href="{{ asset('axa/assets/images/logo.png') }}">
    <meta name="apple-mobile-web-app-title" content="Flatkit">
    <!-- for Chrome on Android, multi-resolution icon of 196x196 -->
    <meta name="mobile-web-app-capable" content="yes">
    <link rel="shortcut icon" sizes="196x196" href="{{ asset('axa/assets/images/logo.png') }}">

    <!-- style -->
    <link rel="stylesheet" href="{{ asset('axa/assets/animate.css/animate.min.css') }}" type="text/css" />
    <link rel="stylesheet" href="{{ asset('axa/assets/glyphicons/glyphicons.css') }}" type="text/css" />
    <link rel="stylesheet" href="{{ asset('axa/assets/font-awesome/css/font-awesome.min.css') }}" type="text/css" />
    <link rel="stylesheet" href="{{ asset('axa/assets/material-design-icons/material-design-icons.css') }}" type="text/css" />

    <link rel="stylesheet" href="{{ asset('axa/assets/bootstrap/dist/css/bootstrap.min.css') }}" type="text/css" />
    <!-- build:css ../assets/styles/app.min.css -->
    <link rel="stylesheet" href="{{ asset('axa/assets/styles/app.css') }}" type="text/css" />
    <!-- endbuild -->
    <link rel="stylesheet" href="{{ asset('axa/assets/fonts/roboto/font.css') }}" type="text/css" />
    <!-- style -->
    <link rel="stylesheet" href="{{ asset('css/admin.css') }}" type="text/css" />
    <link rel="stylesheet" href="{{ asset('axa/libs/jquery/plugins/integration/bootstrap/3/dataTables.bootstrap.css') }}" type="text/css" />

</head>
<body>
<div class="app" id="app">

    <!-- ############ LAYOUT START-->
    {% block left %}
    <!-- aside -->
    {% if is_granted('ROLE_MEMBERSHIP') %}
        {{ include('admin/partials/membershipLeft.htm.twig') }}
    {% elseif is_granted('ROLE_ACTOR_DIRECTOR') %}
        {{ include('admin/partials/actor-directorLeft.htm.twig') }}
    {% elseif is_granted('ROLE_MUSIC_DIRECTOR') %}
        {{ include('admin/partials/music-directorLeft.htm.twig') }}
    {% elseif is_granted('ROLE_ADMINISTRATOR') %}
        {{ include('admin/partials/adminLeft.htm.twig') }}
    {% endif %}

    <!-- / -->
    {% endblock %}
    <!-- content -->
    {% block body %} {% endblock %}
    <!-- / -->


    <!-- ############ LAYOUT END-->

</div>
<!-- build:js scripts/app.html.js -->
<!-- jQuery -->
<script src="{{ asset('axa/libs/jquery/jquery/dist/jquery.js') }}"></script>



<script src="{{ asset('axa/scripts/ui-jp.js') }}"></script>
<script src="{{ asset('axa/scripts/ui-include.js') }}"></script>
<script src="{{ asset('axa/scripts/ui-device.js') }}"></script>
<script src="{{ asset('axa/scripts/ui-form.js') }}"></script>
<script src="{{ asset('axa/scripts/ui-nav.js') }}"></script>
<script src="{{ asset('axa/scripts/ui-screenfull.js') }}"></script>
<script src="{{ asset('axa/scripts/ui-scroll-to.js') }}"></script>
<script src="{{ asset('axa/scripts/ui-toggle-class.js') }}"></script>



<script src="{{ asset('axa/libs/jquery/datatables/media/js/jquery.dataTables.min.js') }}"></script>
<script src="{{ asset('axa/libs/jquery/plugins/integration/bootstrap/3/dataTables.bootstrap.js') }}"></script>

<script type="text/javascript">
$(document).ready(function(){
$('#data').DataTable();
});
</script>
{% block javascripts %}
{% endblock %}
<!-- endbuild -->
</body>
</html>
